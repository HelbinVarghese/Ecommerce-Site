<section>
    <div class="container">
        <table class="table mt-5">
  <thead>
    
    <tr>
      <th scope="col">NO</th>
      <th scope="col">Title</th>
      <th scope="col">Category</th>
      <th scope="col">Description</th>
      <th scope="col">Image</th>
    </tr>
    
  </thead>
  <tbody>
    {{#each orders}}
